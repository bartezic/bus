<% if @params[:one_way] == "true" %>
  $("#trips").html(" <%= escape_javascript(render('tickets/search', :t => @tickets, :p => @params)) %>")
<% else %>
  $("#trips").html(" <%= escape_javascript(render('tickets/search_two', :t_to => @tickets_to, :t_from => @tickets_from, :p => @params)) %>")
<% end %>

